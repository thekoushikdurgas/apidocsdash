      "children": {
        "User Registration": {
          "level": 3,
          "is_last": false,
          "section": {
            "content_text": "Handles user registration processes, allowing creation of new accounts with optional role specification."
          },
          "children": {
            "Generic User Registration": {
              "level": 4,
              "is_last": true,
              "section": {
                "content_text": "Registers a new user in the system. If no specific user type is provided, it defaults to creating a 'customer' account."
              },
              "api_endpoints": [
                {
                  "endpoint": "POST /api/users/register",
                  "request_body": {
                    "email": "user@example.com",
                    "password": "securePassword123",
                    "first_name": "John",
                    "last_name": "Doe",
                    "phone_number": "+1234567890"
                  },
                  "curl_command": "curl -X POST http://localhost:8000/api/users/register -H \"Content-Type: application/json\" -H \"Accept: application/json\" -d '{\"email\": \"user@example.com\", \"password\": \"securePassword123\", \"first_name\": \"John\", \"last_name\": \"Doe\", \"phone_number\": \"+1234567890\"}'",
                  "responses": {
                    "success": {
                      "status_code": 201,
                      "description": "Created",
                      "example": {
                        "message": "User registered successfully. Please check your email to verify your account.",
                        "data": {
                          "user_id": "uuid-generated-by-server",
                          "email": "user@example.com",
                          "first_name": "John",
                          "last_name": "Doe",
                          "phone_number": "+1234567890",
                          "user_type": "customer",
                          "is_active": true,
                          "is_verified": false,
                          "created_at": "YYYY-MM-DDTHH:MM:SSZ",
                          "updated_at": "YYYY-MM-DDTHH:MM:SSZ"
                        }
                      }
                    },
                    "error": {
                      "status_code": 400,
                      "description": "Bad Request",
                      "example": {
                        "error": {
                          "code": "VALIDATION_ERROR",
                          "message": "Input validation failed.",
                          "details": [
                            {
                              "field": "email",
                              "issue": "User with this email already exists."
                            }
                          ]
                        }
                      }
                    }
                  }
                }
              ]
            },
            "Customer Specific Registration": {
              "level": 4,
              "is_last": true,
              "section": {
                "content_text": "Registers a new user with a 'customer' role, including customer-specific profile information."
              },
              "api_endpoints": [
                {
                  "endpoint": "POST /api/users/register/customer",
                  "request_body": {
                    "email": "customer@example.com",
                    "password": "securePassword123",
                    "first_name": "Jane",
                    "last_name": "Smith",
                    "phone_number": "+1987654321",
                    "address": {
                      "street": "123 Customer Lane",
                      "city": "Clientville",
                      "state": "CA",
                      "zip_code": "90210",
                      "country": "USA"
                    }
                  },
                  "curl_command": "curl -X POST http://localhost:8000/api/users/register/customer -H \"Content-Type: application/json\" -H \"Accept: application/json\" -d '{\"email\": \"customer@example.com\", \"password\": \"securePassword123\", \"first_name\": \"Jane\", \"last_name\": \"Smith\", \"phone_number\": \"+1987654321\", \"address\": {\"street\": \"123 Customer Lane\", \"city\": \"Clientville\", \"state\": \"CA\", \"zip_code\": \"90210\", \"country\": \"USA\"}}'",
                  "responses": {
                    "success": {
                      "status_code": 201,
                      "description": "Created",
                      "example": {
                        "message": "User registered successfully. Please check your email to verify your account.",
                        "data": {
                          "user_id": "uuid-generated-by-server",
                          "email": "customer@example.com",
                          "first_name": "Jane",
                          "last_name": "Smith",
                          "phone_number": "+1987654321",
                          "address": {
                            "street": "123 Customer Lane",
                            "city": "Clientville",
                            "state": "CA",
                            "zip_code": "90210",
                            "country": "USA"
                          },
                          "user_type": "customer"
                        }
                      }
                    }
                  }
                }
              ]
            },
            "Provider Specific Registration": {
              "level": 4,
              "is_last": true,
              "section": {
                "content_text": "Registers a new user with a 'provider' role, including provider-specific details like company name and service categories."
              },
              "api_endpoints": [
                {
                  "endpoint": "POST /api/users/register/provider",
                  "request_body": {
                    "email": "provider@example.com",
                    "password": "securePasswordProvider123",
                    "first_name": "Service",
                    "last_name": "Pro",
                    "phone_number": "+1555123456",
                    "company_name": "Pro Services Ltd.",
                    "service_categories": [
                      "plumbing",
                      "electrical"
                    ],
                    "business_license_id": "BIZLICENSE789",
                    "address": {
                      "street": "456 Provider Ave",
                      "city": "Workville",
                      "state": "NY",
                      "zip_code": "10001",
                      "country": "USA"
                    }
                  },
                  "curl_command": "curl -X POST http://localhost:8000/api/users/register/provider -H \"Content-Type: application/json\" -H \"Accept: application/json\" -d '{\"email\": \"provider@example.com\", \"password\": \"securePasswordProvider123\", \"first_name\": \"Service\", \"last_name\": \"Pro\", \"phone_number\": \"+1555123456\", \"company_name\": \"Pro Services Ltd.\", \"service_categories\": [\"plumbing\", \"electrical\"], \"business_license_id\": \"BIZLICENSE789\", \"address\": {\"street\": \"456 Provider Ave\", \"city\": \"Workville\", \"state\": \"NY\", \"zip_code\": \"10001\", \"country\": \"USA\"}}'",
                  "responses": {
                    "success": {
                      "status_code": 201,
                      "description": "Created",
                      "example": {
                        "message": "Provider user registered successfully",
                        "data": {
                          "user_id": "uuid-generated-by-server",
                          "email": "provider@example.com",
                          "first_name": "Service",
                          "last_name": "Pro",
                          "phone_number": "+1555123456",
                          "user_type": "provider",
                          "company_name": "Pro Services Ltd.",
                          "service_categories": [
                            "plumbing",
                            "electrical"
                          ],
                          "business_license_id": "BIZLICENSE789",
                          "address": {
                            "street": "456 Provider Ave",
                            "city": "Workville",
                            "state": "NY",
                            "zip_code": "10001",
                            "country": "USA"
                          }
                        }
                      }
                    }
                  }
                }
              ]
            },
            "Admin Specific Registration": {
              "level": 4,
              "is_last": true,
              "section": {
                "content_text": "Registers a new admin user in the system, typically requiring elevated permissions."
              },
              "api_endpoints": [
                {
                  "endpoint": "POST /api/admins/register",
                  "request_body": {
                    "username": "new_admin_user",
                    "email": "admin@example.com",
                    "password": "securePassword123",
                    "first_name": "Admin",
                    "last_name": "User",
                    "phone_number": "1234567890",
                    "is_staff": true,
                    "is_superuser": false
                  },
                  "curl_command": "curl -X POST http://localhost:8000/api/admins/register -H \"Content-Type: application/json\" -H \"Authorization: Bearer <SUPER_ADMIN_ACCESS_TOKEN>\" -d '{\"username\": \"new_admin_user\", \"email\": \"admin@example.com\", \"password\": \"securePassword123\", \"first_name\": \"Admin\", \"last_name\": \"User\", \"phone_number\": \"1234567890\", \"is_staff\": true, \"is_superuser\": false}'",
                  "responses": {
                    "success": {
                      "status_code": 201,
                      "description": "Created",
                      "example": {
                        "message": "Admin user registered successfully",
                        "data": {
                          "user_id": "uuid-generated-by-server",
                          "email": "admin@example.com",
                          "first_name": "Admin",
                          "last_name": "User",
                          "phone_number": "1234567890",
                          "user_type": "admin",
                          "is_staff": true,
                          "is_superuser": false
                        }
                      }
                    },
                    "error": {
                      "status_code": 400,
                      "description": "Bad Request",
                      "example": {
                        "detail": {
                          "email": [
                            "admin with this email already exists."
                          ],
                          "username": [
                            "A user with that username already exists."
                          ]
                        }
                      }
                    }
                  }
                }
              ]
            }
          }
        },
        "User Logout": {
          "level": 3,
          "is_last": true,
          "section": {
            "content_text": "Logs out the currently authenticated user by invalidating their session or access token."
          },
          "api_endpoints": [
            {
              "endpoint": "POST /api/users/logout",
              "request_body": {},
              "curl_command": "curl -X POST http://localhost:8000/api/users/logout -H \"Authorization: Bearer <USER_ACCESS_TOKEN>\" -H \"Content-Type: application/json\" -d '{}'",
              "responses": {
                "success": {
                  "status_code": 200,
                  "description": "OK",
                  "example": {
                    "message": "Successfully logged out."
                  }
                },
                "error": {
                  "status_code": 401,
                  "description": "Unauthorized",
                  "example": {
                    "detail": "Authentication credentials were not provided."
                  }
                }
              }
            }
          ]
        },
        "Access Token Management": {
          "level": 3,
          "is_last": false,
          "section": {
            "content_text": "Endpoints for managing user access tokens, such as listing, revoking, or refreshing them."
          },
          "children": {
            "List User's Access Tokens": {
              "level": 4,
              "is_last": false,
              "section": {
                "content_text": "Retrieves a list of active access tokens or sessions for the currently authenticated user."
              },
              "api_endpoints": [
                {
                  "endpoint": "GET /api/users/tokens",
                  "request_body": null,
                  "curl_command": "curl -X GET http://localhost:8000/api/users/tokens -H \"Authorization: Bearer <USER_ACCESS_TOKEN>\"",
                  "responses": {
                    "success": {
                      "status_code": 200,
                      "description": "OK",
                      "example": {
                        "tokens": [
                          {
                            "id": "token_id_1",
                            "device_info": "Chrome on Windows 10",
                            "ip_address": "192.168.1.10",
                            "last_used_at": "YYYY-MM-DDTHH:MM:SSZ",
                            "created_at": "YYYY-MM-DDTHH:MM:SSZ"
                          }
                        ],
                        "pagination": {
                          "count": 2,
                          "next": null,
                          "previous": null
                        }
                      }
                    },
                    "error": {
                      "status_code": 401,
                      "description": "Unauthorized",
                      "example": {
                        "detail": "Authentication credentials were not provided."
                      }
                    }
                  }
                }
              ]
            },
            "Revoke Specific Access Token": {
              "level": 4,
              "is_last": false,
              "section": {
                "content_text": "Allows a user to revoke a specific access token or session."
              },
              "api_endpoints": [
                {
                  "endpoint": "DELETE /api/users/tokens/{token_id}",
                  "path_parameters": {
                    "token_id": "The unique identifier of the access token/session to be revoked."
                  },
                  "request_body": null,
                  "curl_command": "curl -X DELETE http://localhost:8000/api/users/tokens/token_id_to_revoke -H \"Authorization: Bearer <USER_ACCESS_TOKEN>\"",
                  "responses": {
                    "success": {
                      "status_code": 204,
                      "description": "No Content"
                    },
                    "error": {
                      "status_code": 401,
                      "description": "Unauthorized",
                      "example": {
                        "detail": "Authentication credentials were not provided."
                      }
                    }
                  }
                }
              ]
            },
            "Refresh JWT Access Token": {
              "level": 4,
              "is_last": true,
              "section": {
                "content_text": "Allows a client to obtain a new JWT access token using a valid refresh token."
              },
              "api_endpoints": [
                {
                  "endpoint": "POST /api/token/refresh/",
                  "request_body": {
                    "refresh": "<USER_REFRESH_TOKEN>"
                  },
                  "curl_command": "curl -X POST http://localhost:8000/api/token/refresh/ -H \"Content-Type: application/json\" -d '{\"refresh\": \"<USER_REFRESH_TOKEN>\"}'",
                  "responses": {
                    "success": {
                      "status_code": 200,
                      "description": "OK",
                      "example": {
                        "access": "<NEW_ACCESS_TOKEN>",
                        "refresh": "<MAYBE_NEW_REFRESH_TOKEN>"
                      }
                    },
                    "error": {
                      "status_code": 401,
                      "description": "Unauthorized",
                      "example": {
                        "detail": "Token is invalid or expired"
                      }
                    }
                  }
                }
              ]
            }
          }
        }
      }
